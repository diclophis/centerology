<% content_for :tagline do %>
  <%=link_to("Findings tagged with #{@tag}", {:controller => :findings, :action => :tagged, :id => @tag})%>
<% end %>
<ul class="findings">
  <% @findings.each { |finding| %>
    <li>
      <%=link_to(image_tag(finding.image.icon_permalink), image_url(finding.image.permalink))%>
    </li>
  <% } %>
</ul>
<ul>
  <li>
    <%=link_to(:return, root_url)%>
  </li>
</ul>
